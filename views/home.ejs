    <body>
        <div id="login">
            <h3 class="text-center text-white pt-5">Login form</h3>
            <div class="container">
                <div id="login-row" class="row justify-content-center align-items-center">
                    <div id="login-column" class="card col-md-6">
                        <div id="login-box" class="card-body col-md-12">
                            <form id="login-form" class="form" action="" method="post">
                                <h3 class="text-center text-info">Login</h3>
                                <div class="form-group">
                                    <label for="username" class="text-info">Username:</label><br>
                                    <input type="text" name="username" id="username" class="form-control">
                                </div>
                                <div class="form-group" style="margin-bottom: 18px;">
                                    <label for="password" class="text-info">Password:</label><br>
                                    <input type="text" name="password" id="password" class="form-control">
                                </div>
                                <div class="form-group" style="display: flex; justify-content: space-between;">
                                    <button class="btn btn-primary btn-lg btn-block">Logar</button>
                                    <a href="#" class="btn btn-primary btn-lg btn-block" style="color: white; text-decoration: none;">Registrar</a>
                                </div>
                                <div id="register-link" class="text-right" style="margin-top: 10px;">
                                    
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
